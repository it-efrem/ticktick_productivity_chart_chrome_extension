{
  "name": "TickTick productivity chart",
  "description": "Track your productivity with a bar chart across lists",
  "version": "1.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://ticktick.com/"
  ],
  "action": {
    "default_title": "TickTick productivity chart",
    "default_icon": "favicon.png",
    "default_popup": "index.html"
  }
}
